<Window x:Class="ViinikellariT4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViinikellariT4"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="200">
    <Grid>
        <StackPanel>
            <Button x:Name="btnXML1" Content="XMLDataProvider demo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="170" Click="btnXML1_Click"/>
            <Button x:Name="btnXML2" Content="XMLDocument demo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="170"/>
            <Button x:Name="btnXML3" Content="XDocument demo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="170" Click="btnXML3_Click"/>
            <Button x:Name="btnXML4" Content="DataSet demo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="170" Click="btnXML4_Click"/>
        </StackPanel>
    </Grid>
</Window>
